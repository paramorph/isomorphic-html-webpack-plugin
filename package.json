{
  "name": "isomorphic-html-webpack-plugin",
  "version": "0.1.0",
  "description": "Static HTML generator plugin with support for webpack code splitting.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib #",
    "compile-ts": "tsc",
    "compile-coffee": "coffee -c -m -o lib src",
    "compile": "npm run compile-ts && npm run compile-coffee",
    "test": "mocha --use_strict lib/test/harness.js $(find lib -name \"*.spec.js\")",
    "copy-package.json": "mkdir -p lib/ && cp package.json lib/",
    "copy": "npm run copy-package.json",
    "build": "npm run compile && npm run copy && npm run test",
    "clean-build": "npm run clean && npm run build",
    "deploy": "npm run clean-build && npm publish lib"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/paramorph/isomorphic-html-webpack-plugin.git"
  },
  "keywords": [
    "static",
    "html",
    "webpack",
    "plugin",
    "isomorphic",
    "generator",
    "code-splitting"
  ],
  "author": "Maciej Cha≈Çapuk <maciej@chalapuk.pl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paramorph/isomorphic-html-webpack-plugin/issues"
  },
  "homepage": "https://github.com/paramorph/isomorphic-html-webpack-plugin#readme",
  "devDependencies": {
    "@types/webpack": "^4.4.9",
    "coffeescript": "^2.3.1",
    "mocha": "^5.2.0",
    "should": "^13.2.3",
    "typescript": "^3.0.1",
    "webpack": "^4.16.4"
  }
}
